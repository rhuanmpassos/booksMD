{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/booksmd/browser",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://52.87.194.234:8000/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

